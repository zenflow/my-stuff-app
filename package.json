{
  "name": "my-stuff-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db-clean": "docker volume rm my_stuff_app_pg_data",
    "db": "docker run --name my_stuff_app_pg -it --rm --env POSTGRES_PASSWORD=password --volume my_stuff_app_pg_data:/var/lib/postgresql/data --publish 5432:5432 postgres:14.3-alpine3.16",
    "db-init": "docker exec my_stuff_app_pg createdb my_stuff_app --username=postgres && docker exec my_stuff_app_pg createdb my_stuff_app_shadow --username=postgres",
    "db-migrate": "dotenv graphile-migrate migrate",
    "db-migrate-watch": "dotenv graphile-migrate watch",
    "db-migrate-commit": "dotenv graphile-migrate commit",
    "db-migrate-uncommit": "dotenv graphile-migrate uncommit",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint-fix": "yarn lint --fix"
  },
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "6.1.0",
    "@next-auth/sequelize-adapter": "1.0.6",
    "graphile-migrate": "1.4.0",
    "next": "13.0.1-canary.1",
    "next-auth": "4.15.0",
    "pg": "^8.8.0",
    "pg-hstore": "2.3.4",
    "postgraphile": "4.12.11",
    "postgraphile-plugin-connection-filter": "2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sequelize": "6.25.3"
  },
  "devDependencies": {
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "dotenv-cli": "6.0.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.1-canary.1",
    "eslint-config-zenflow": "4.0.0",
    "typescript": "4.8.4"
  }
}
